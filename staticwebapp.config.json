{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET",
                "POST",
                "OPTIONS"
            ],
            "allowedRoles": [
                "anonymous",
                "authenticated"
            ]
        }
    ],
    "globalHeaders": {
        "content-security-policy": "default-src 'self' https: 'unsafe-inline' 'unsafe-eval'; img-src 'self' data: https:; font-src 'self' https: data:;"
    },
    "mimeTypes": {
        ".json": "application/json"
    }
}